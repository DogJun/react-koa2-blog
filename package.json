      "eslint --fix src/**/*",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DogJun/react-koa2-blog.git"
  },
  "keywords": [
    "react",
    "hooks",
    "koa2",
    "mysql"
