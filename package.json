{
  "name": "react-koa2-blog",
  "version": "1.0.0",
  "description": "my blog with React & Koa2",
  "main": "index.js",
  "scripts": {
    "client:dev": "scripty",
    "client:build": "scripty",
    "server:dev": "scripty",
    "server:start": "scripty",
    "commit": "git cz",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "lint:fix": "eslint --ext=jsx,ts,tsx src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "prettier .prettierrc.js --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DogJun/react-koa2-blog.git"
  },
  "keywords": [
    "react",
    "hooks",
    "koa2",
    "mysql"
  ],
  "author": "DogJun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DogJun/react-koa2-blog/issues"
  },
  "homepage": "https://github.com/DogJun/react-koa2-blog#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/react": "^16.9.3",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "commitlint-config-gitmoji": "^1.0.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cz-emoji": "^1.1.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "scripty": "^1.9.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2",
    "yargs-parser": "^14.0.0"
  },
  "dependencies": {
    "awilix": "^4.2.3",
    "awilix-koa": "^4.0.0",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-koa-utils": "^1.0.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-swig": "^2.2.1",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "log4js": "^6.1.0",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "node-fetch": "^2.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.0",
    "reflect-metadata": "^0.1.13"
  }
}
